{
  "name": "zenjson",
  "version": "1.0.3",
  "description": "Transform custom types into JSON-compatible data.",
  "keywords": [
    "json",
    "parse",
    "serialize"
  ],
  "homepage": "https://github.com/etienne-dldc/zenjson#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/zenjson/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/zenjson.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "codecov": "codecov",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "test:watch-cov": "jest --watch --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "codecov": "^3.8.3",
    "denoify": "^0.10.6",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.7",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
