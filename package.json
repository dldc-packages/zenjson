{
  "name": "zenjson",
  "version": "1.0.0",
  "description": "Transform custom types into JSON-compatible data.",
  "keywords": [
    "json",
    "parse",
    "serialize"
  ],
  "homepage": "https://github.com/etienne-dldc/zenjson#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/zenjson/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/zenjson.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/zenjson.es.js",
      "require": "./dist/zenjson.cjs.js"
    }
  },
  "main": "./dist/zenjson.cjs.js",
  "module": "./dist/zenjson.es.js",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build && yarn build:types && denoify",
    "build:types": "tsc -p ./tsconfig.build.json",
    "codecov": "codecov",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "test:watch-cov": "jest --watch --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "codecov": "^3.8.3",
    "denoify": "^0.9.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.0",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
